                        <span class="section__title--type">
                            ({{this.context.type}})
                        </span>
                    </h3>
                    <div class="section__information">
                        <div class="section__code-wrapper" onclick="openCode('font',{{@index}})">
                            <pre id="font-{{@index}}" aria-expanded="false" data-type="{{this.context.type}}">

                                <span class="section__code-wrapper--title">
                                    {{#if (isMixin this.context.type)}}
                                    <span>${{this.context.type}}</span>
                                    <span>{{this.context.name}}(</span>
                                    <span>{{#each this.parameter}}{{#if @last}}{{this.name}}{{else}}${{this.name}}, {{/if}}{{/each}}</span>)
                                    {{else if (isCSS this.context.type)}}
                                        <span>{{this.context.name}}:{</span>
                                        {{#if @last}}
                                        <span>{{this.context.value}} <p>}</p></span>
                                        {{else}}
                                        <p>{{this.context.value}}</p>
                                        {{/if}}
                                    {{else}}
                                    <span>${{this.context.name}}:</span>
                                    <span> {{this.context.value}}</span>
                                    {{/if}}
                                </span>
                                <span id="font-content-{{@index}}" class="section__code-wrapper--code">
                                    <span>{{this.context.code}}</span>
                                </span>
                            </pre>

                        </div>
