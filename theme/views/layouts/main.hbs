                        </h3>
                        {{#each this.example}}
                        <div class="section__code-wrapper example">
                            <pre>
                                <span>{{this.code}}</span>
                            </pre>
                        </div>
                        {{/each}}
                        {{#if (isDefined this.require)}}
                        <h3 class="section__subtitle">
                            Dependencias
                        </h3>
                        {{#each this.require}}
                        <li class="section__require">
                            <span>
                                [{{this.type}}]
                            </span>
                            <span>
                                {{this.name}}
                            </span>
                        </li>
                        {{/each}}
                        {{/if}}
                    </div>
                    {{/each}}
                </div>
            </div>
            <div class="section" id="{{posts.functions.0.group}}">
                <div class="section__header">
                    <h2>{{posts.functions.0.group}}</h2>
                </div>
                <div class="section__body">
                    {{#each posts.functions}}
                    <h3 class="section__title">
                        <span class="section__title--name">
                            {{this.context.name}}
                        </span>
                        <span class="section__title--type">
                            ({{this.context.type}})
                        </span>
                    </h3>
                    <div class="section__information">
                        <div class="section__code-wrapper">
                            <pre id="functions-{{@index}}" aria-expanded="false" data-type="{{this.context.type}}">
                                <span class="section__code-wrapper--title">
                                    {{#if (isMixin this.context.type)}}
                                    <span>@{{this.context.type}}</span>
                                    <span>{{this.context.name}}(</span>
                                    <span>{{#each this.parameter}}{{#if @last}}{{this.name}}{{else}}${{this.name}}, {{/if}}{{/each}}</span>)
                                    {{else}}
                                    {{this.context.value}}
                                    {{/if}}
                                </span>
                                <span id="functions-content-{{@index}}" class="section__code-wrapper--code">
                                    {{this.context.code}}
                                </span>
                            </pre>

                        </div>
                        <h3 class="section__subtitle">
                            Descripción
                        </h3>
                        <p class="section__description">
                            {{this.description}}
                        </p>

                        {{#if (isDefined this.parameter) }}
                        <h3 class="section__subtitle">
                            Parametros
                        </h3>
                        <table class="section__parameters">
                            <thead>
                                <tr>
                                    <th class="section__parameters--name">Nombre</th>
                                    <th class="section__parameters--description">Descripción</th>
                                    <th class="section__parameters--type">Tipo</th>
                                    <th class="section__parameters--default">Valor por defecto</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each this.parameter}}
                                <tr>
                                    <td class="section__parameters--name">${{this.name}}</td>
                                    <td class="section__parameters--description">{{this.description}}</td>
                                    <td class="section__parameters--type">{{this.type}}</td>
                                    <td class="section__parameters--default">{{this.default}}</td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>

                        {{/if}}
                        <h3 class="section__subtitle">
                            Example
                        </h3>
                        {{#each this.example}}
                        <div class="section__code-wrapper example">
                            <pre>
                                <span>{{this.code}}</span>
                            </pre>
                        </div>
                        {{/each}}
                        {{#if (isDefined this.require)}}
                        <h3 class="section__subtitle">
                            Dependencias
                        </h3>
                        {{#each this.require}}
                        <li class="section__require">
                            <span>
                                [{{this.type}}]
                            </span>
                            <span>
                                {{this.name}}
                            </span>
                        </li>
                        {{/each}}
                        {{/if}}
                    </div>
                    {{/each}}
                </div>
            </div>

        </main>

    </div>
    <script src="scripts/script.js"></script>
</body>

</html>